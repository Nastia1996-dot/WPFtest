    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Localization\Models\CompanyVehicleLoc.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>CompanyVehicleLoc.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Localization\Models\PersonalComputerLoc.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>PersonalComputerLoc.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>


